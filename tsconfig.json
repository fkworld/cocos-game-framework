{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "lib": [
            "DOM",
            "ESNext",
        ],
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "downlevelIteration": true,
        "esModuleInterop": true,
    },
    "exclude": [
        "node_modules",
        "library",
        "local",
        "temp",
        "build",
        "settings"
    ]
}
